{
  "name": "barrels-client",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://app.barrels.wine",
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules/ ./src/index.scss ./src/index.css",
    "watch-css": "node-sass-chokidar --include-path ./node_modules/ ./src/index.scss ./src/index.css --watch",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "cp ./.env ./.env.production && npm run build && mkdir -p ./build && echo 'app.barrels.wine' > ./build/CNAME",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.5",
    "connected-react-router": "^4.3.0",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "i18n-iso-countries": "^3.7.3",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.2.2",
    "normalizr": "^3.2.4",
    "npm-run-all": "^4.1.2",
    "react": "^16.3.1",
    "react-data-grid": "^4.0.7",
    "react-datetime": "^2.15.0",
    "react-dom": "^16.3.1",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-select": "^2.0.0",
    "react-toastify": "^4.1.0",
    "react-transition-group": "^2.3.1",
    "react-virtualized-select": "^3.1.3",
    "reactstrap": "^6.3.1",
    "redux": "^3.7.2",
    "redux-auth-wrapper": "^2.0.2",
    "redux-axios-middleware": "^4.0.0",
    "redux-form": "^7.3.0",
    "redux-persist": "^5.9.1",
    "redux-thunk": "^2.2.0",
    "simple-line-icons": "^2.4.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0"
  }
  
}
